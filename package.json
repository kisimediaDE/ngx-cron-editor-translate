  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-cron-editor-translate --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copyhtml": "copyfiles 'src/app/cron-editor/*.html' ./components/cron-editor/",
